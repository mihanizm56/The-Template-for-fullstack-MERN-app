{
  "name": "test-auth",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "client": "cd client && npm start",
    "server": "cd server && npm start",
    "start": "concurrently --kill-others-on-fail \"npm run client\" \"npm run server:dev\"",
    "production": "echo 0",
    "stop": "taskkill /F /IM node.exe",
    "install:main": "npm i",
    "install:server": "cd server && npm i",
    "install:client": "cd client && npm i",
    "install:all": "npm run install:server && npm run install:client && npm run install:main",
    "push": "git add . && git commit -m default && git push"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
}
